	partition Date_Table = calculated
		mode: import
		source =
				
				VAR StartDate = DATE(2016, 1, 1)
				VAR EndDate = DATE(2026, 12, 31)
				RETURN
				ADDCOLUMNS (
				    CALENDAR(StartDate, EndDate),
				    "Year", YEAR([Date]),
				    "Quarter", "Q" & FORMAT([Date], "Q"),
				    "MonthNumber", MONTH([Date]),
				    "MonthName", FORMAT([Date], "MMMM"),
				    "YearMonth", FORMAT([Date], "YYYY-MM"),
				    "DayName", FORMAT([Date], "DDDD"),
				    "DayOfWeekNumber", WEEKDAY([Date], 2),
				    "StartOfYear",  DATE( YEAR([Date]), 1, 1 ),
				    "StartOfMonth", DATE( YEAR([Date]), MONTH([Date]), 1 ),
				    "StartOfWeek",  [Date] - WEEKDAY([Date], 2) + 1  -- 基于周一为一周开始的逻辑
				)

	annotation PBI_Id = cd1655c17308450580a0ce057db2703d

